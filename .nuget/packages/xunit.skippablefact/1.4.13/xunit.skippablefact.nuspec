<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Xunit.SkippableFact</id>
    <version>1.4.13</version>
    <title>Dynamic test skipping for Xunit</title>
    <authors>Andrew Arnott</authors>
    <owners>Andrew Arnott</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MS-PL</license>
    <licenseUrl>https://licenses.nuget.org/MS-PL</licenseUrl>
    <projectUrl>https://github.com/AArnott/Xunit.SkippableFact</projectUrl>
    <description>Make your Xunit test methods self-determine to report a "skipped" result. Useful for such cases as "not supported on this platform" results or other environmental inputs.</description>
    <releaseNotes>https://github.com/AArnott/Xunit.SkippableFact/releases/tag/v1.4.13</releaseNotes>
    <tags>xunit testing skipping</tags>
    <repository type="git" url="https://github.com/AArnott/Xunit.SkippableFact" commit="6702cf0198e2ab9da1a5bb3bea2e230cfae6dd8b" />
    <dependencies>
      <group targetFramework=".NETFramework4.5">
        <dependency id="Validation" version="2.4.18" exclude="Compile,Build,Analyzers" />
        <dependency id="xunit.extensibility.execution" version="2.1.0" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETFramework4.5.2">
        <dependency id="Validation" version="2.4.18" exclude="Compile,Build,Analyzers" />
        <dependency id="xunit.extensibility.execution" version="2.4.0" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Validation" version="2.4.18" exclude="Compile,Build,Analyzers" />
        <dependency id="xunit.extensibility.execution" version="2.4.0" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
</package>